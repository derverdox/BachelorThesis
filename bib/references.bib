  title        = {Defining the Ethereum Virtual Machine for Interactive Theorem Provers},
  author       = {Hirai, Yoichi},
  year         = 2017,
  booktitle    = {Financial Cryptography and Data Security},
  publisher    = {Springer International Publishing},
  address      = {Cham},
